{
    "name": "manish-pareek/laravel-attributes",
    "description": "Flexible EAV (Entity–Attribute–Value) model for Laravel 10, 11, and 12.",
    "keywords": [
        "laravel",
        "attributes",
        "eav",
        "rinvex",
        "custom-fields"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Manish Pareek",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": ">=10.0 <13.0",
        "illuminate/database": ">=10.0 <13.0",
        "illuminate/events": ">=10.0 <13.0",
        "illuminate/console": ">=10.0 <13.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": ">=8.0 <11.0"
    },
    "autoload": {
        "psr-4": {
            "Rinvex\\Attributes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rinvex\\Attributes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rinvex\\Attributes\\Providers\\AttributesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
